{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "env": {
    "test": {
      "presets": [["@babel/preset-env", { "targets": { "node": "current" } }]]
    }
  },
  "plugins": [
    // Your existing plugins
    [
      "module-resolver",
      {
        "alias": {
          "countup.js": "./node_modules/countup.js/dist/countUp.umd.js"
        }
      }
    ]
  ]
}
